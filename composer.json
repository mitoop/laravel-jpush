{
    "name": "mitoop/laravel-jpush",
    "description": "laravel jpush",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mitoop",
            "email": "me@mitoop.com"
        }
    ],
    "require": {
        "illuminate/queue": "^9|^10",
        "illuminate/support": "^9|^10",
        "jpush/jpush": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Mitoop\\JPush\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "JPush": "Mitoop\\JPush\\JPushServiceFacade"
            },
            "providers": [
                "Mitoop\\JPush\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
