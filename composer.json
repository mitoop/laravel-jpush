{
    "name": "mitoop/laravel-jpush",
    "description": "laravel jpush",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "徐夏冰",
            "email": "037333@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
        "jpush/jpush": "^3.6"
    },
    "autoload": {
        "psr-4": {
